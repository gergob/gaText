{
  "manifest_version": 2,
  "name": "gaText",
  "short_name": "gaText",
  "description": "A simple text editor.",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    {
      "fileSystem": [
        "write"
      ]
    },
    "storage",
    "webview"
  ]
}
